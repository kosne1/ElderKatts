// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model nft_card_codes {
  nft_card_code        String     @default(cuid()) @id
  received_nft    Boolean @default(false)
 

}
model users {
  tg_nickname     String    
  received_nft    Boolean @default(false)
  twitter_nickname String
  nft_card_code        String     @default(cuid()) @id

}